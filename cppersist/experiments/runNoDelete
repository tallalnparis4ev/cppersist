#!/bin/bash
#$1 means = number of runs
#$2 means = recursion num input
#$3 means = iterative num input
#$4 means = seq/wrep/worep

#5 input, use cppersist, recursive, keep cache, seed, worep/wrep/seq 
#5 1 1 0 0 worep/wrep/seq

# Recursive
rm -rf persist
for i in $(seq "$1")
do
  /usr/bin/time -p -o RecWCache.out ./cppersist $2 1 1 1 $i $4
done

# Iterative
rm -rf persist
for i in $(seq "$1")
do
  /usr/bin/time -p -o IterWCache.out ./cppersist $3 1 0 1 $i $4
done