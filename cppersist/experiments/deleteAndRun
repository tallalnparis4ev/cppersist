#!/bin/bash
#$1 = number of runs
#$2 = recursion num input
#$3 = iterative num input
#$4 = seq/wrep/worep

#5 input, use cppersist, recursive, keep cache, seed, worep/wrep/seq 
#5 1 1 0 0 worep/wrep/seq

#Recursive
for i in $(seq "$1")
do
  rm -rf persist #delete cache before starting run
  ./cppersist $2 1 1 0 $i $4
done

#Iterative
for i in $(seq "$1")
do
  rm -rf persist #delete cache before starting run
  ./cppersist $3 1 0 0 $i $4
done


